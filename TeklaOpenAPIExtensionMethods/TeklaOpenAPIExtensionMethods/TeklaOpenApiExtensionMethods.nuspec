<?xml version="1.0" encoding="utf-8"?>
<package >
	<metadata>
		<id>TeklaOpenApiExtensionMethods</id>
		<version>4.0.0</version>
		<title>TeklaOpenApiExtensionMethods</title>
		<authors>Dawid Dyrcz</authors>
		<owners>Dawid Dyrcz</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/dawiddyrcz/Tekla-Open-Api-Extension-Methods</projectUrl>
